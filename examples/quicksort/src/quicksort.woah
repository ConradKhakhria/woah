mod Quicksort
    fun quicksort(xs: [int]) [int] =
        if xs.len() < 2
            return xs

        var lowers: [int] = []
        var uppers: [int] = []

        for x in xs
            if x < xs[0]
                lowers.push(x)
            else
                uppers.push(x)

        quicksort(lowers).append(quicksort(uppers))


    fun main() = std.io.println(quicksort([9, 5, 7, 3, 5, 1, 10]))
